{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:newDev": "rimraf ./build && NODE_ENV=development webpack --env development --progress --hide-modules --config ./webpack/webpack.client.babel.js",
    "start:newProd": "rimraf ./build && NODE_ENV=production webpack --env production --progress --hide-modules --config ./webpack/webpack.client.babel.js",
    "start:newStage": "rimraf ./build && NODE_ENV=staging webpack --env staging --progress --hide-modules --config ./webpack/webpack.client.babel.js",
    "start": "better-npm-run start",
    "start:staging": "yarn clean:build && yarn build:staging && yarn start:server",
    "start:production": "yarn clean:build && yarn build:production && yarn start:server",
    "start:server": "better-npm-run start:server",
    "build:staging": "yarn client:staging && yarn build:server",
    "build:production": "yarn client:production && yarn build:server",
    "client:staging": "better-npm-run client:staging",
    "client:production": "better-npm-run client:production",
    "build:server": "better-npm-run build:server",
    "clean:build": "better-npm-run clean:build",
    "lint:js": "eslint ./src ./index.js",
    "lint:style": "stylelint \"./src/**/**/*.scss\" --syntax scss"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon ./index.js --exec babel-node",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 4000
      }
    },
    "start:server": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "client:staging": {
      "command": "webpack --progress --hide-modules --config ./webpack/webpack.client.babel.js",
      "env": {
        "NODE_ENV": "staging"
      }
    },
    "client:production": {
      "command": "webpack --progress --hide-modules --config ./webpack/webpack.client.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "webpack --progress --hide-modules --config ./webpack/webpack.server.babel.js"
    },
    "clean:build": {
      "command": "rimraf ./build"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "happypack": "^4.0.0-beta.2",
    "helmet": "^3.8.1",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "opn": "^5.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
