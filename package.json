{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 3, React boilerplate for universal web application",
  "main": "index.js",
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaunKT/webpack-starter-kit"
  },
  "keywords": [
    "hmr",
    "webpack",
    "SASS",
    "react",
    "React Router"
  ],
  "author": "Shaun Townsend (https://github.com/ShaunKT)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShaunKT/webpack-starter-kit/issues"
  },
  "homepage": "https://github.com/ShaunKT/webpack-starter-kit",
  "scripts": {
    "start": "better-npm-run build:development NODE_ENV=development",
    "start:wds": "webpack-dev-server --config ./webpack/webpack.client.babel.js",
    "build:production": "yarn clean && better-npm-run client:production && better-npm-run server",
    "build:server": "better-npm-run clean:server && better-npm-run server",
    "run:node": "better-npm-run start:node",
    "clean": "better-npm-run clean"
  },
  "betterScripts": {
    "build:development": {
      "command": "nodemon --ignore build --exec babel-node ./src/server/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 4000
      }
    },
    "start:node": {
      "command": "node ./build/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "client:production": {
      "command": "webpack --env production --progress --hide-modules --config ./webpack/webpack.client.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "server": {
      "command": "webpack --progress --hide-modules --config ./webpack/webpack.server.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean:server": {
      "command": "rimraf ./build/server.js"
    },
    "clean": {
      "command": "rimraf ./build"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "glob": "^7.1.2",
    "helmet": "^3.8.1",
    "history": "^4.6.3",
    "http-parser-js": "^0.4.6",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "opn": "^5.1.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-cookie": "^2.1.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-assets-import-to-string": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "happypack": "^4.0.0-beta.5",
    "image-webpack-loader": "^3.3.1",
    "imports-loader": "^0.7.1",
    "nock": "^9.0.14",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss": "^6.0.9",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "susy": "2.2.12",
