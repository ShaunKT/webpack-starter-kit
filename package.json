{
  "name": "webpack-react-starter-kit",
  "version": "1.0.0",
  "description": "A basic Webpack 2, React, React Router, Sass starter kit",
  "main": "index.js",
  "author": "Shaun Townsend",
  "license": "MIT",
  "keywords": [
    "hmr",
    "webpack",
    "SASS",
    "react",
    "React Router"
  ],
  "scripts": {
    "start:wds": "webpack-dev-server --env development",
    "start:dev": "nodemon --ignore dist --exec babel-node src/server",
    "production": "rimraf dist && cross-env NODE_ENV=production webpack -p --env production",
    "staging": "rimraf dist && cross-env NODE_ENV=production webpack -p --env staging",
    "stats": "webpack --env production --profile --json > stats.json",
    "js:lint": "eslint src/ webpack.*.js --cache",
    "css:lint": "stylelint src/styles/**/**/*.scss"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.51.0",
    "glob": "^7.1.2",
    "happypack": "^4.0.0-beta.1",
    "html-webpack-plugin": "^2.29.0",
    "i": "^0.3.5",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "npm": "^5.3.0",
    "postcss": "^6.0.8",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint-webpack-plugin": "^0.9.0",
    "uglify-js": "^3.0.26",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "cross-env": "^5.0.1",
    "cssnano": "^3.10.0",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "stylelint": "^8.0.0"
  }
}